{
  "name": "@derelict/backend",
  "version": "0.1.0",
  "private": true,
  "description": "Discord bot and Lambda functions for Derelict",
  "main": "./dist/backend/index.js",
  "types": "./dist/backend/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/backend/index.d.ts",
      "default": "./dist/backend/index.js"
    }
