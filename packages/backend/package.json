    "@derelict/shared": "workspace:*",
    "@trpc/server": "^11.9.0",
    "discord.js": "^14.14.1",
    "electrodb": "^3.5.3",
    "jsonwebtoken": "^9.0.3",
    "ulid": "^3.0.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@aws-lambda-powertools/logger": "^2.30.2",
    "@types/aws-lambda": "^8.10.160",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
